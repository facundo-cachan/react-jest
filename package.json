{
  "name": "react-jest-test",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html,ts,tsx}": [
      "npx prettier --write ."
    ],
    "*": [
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn check-all"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "https": "set HTTPS=true&&npm start",
    "build": "react-scripts build",
    "test": "NODE_ENV=test jest --env=jsdom --watchAll",
    "test:coverage": "jest --env=jsdom --watchAll --coverage",
    "eject": "react-scripts eject",
    "lockfile": "yarn install --frozen-lockfile",
    "clean": "rm -rf build;rm -rf coverage",
    "prepare": "husky install .config/husky",
    "check-format": "prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "check-all": "yarn prettier && yarn lint && yarn check-format && yarn check-types",
    "lint": "lint-staged",
    "eslint": "eslint --quiet --fix",
    "prettier": "prettier --write .",
    "local": "cp .env.local .env.development && NODE_ENV=development yarn build && yarn start"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.9",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "babel-jest": "27.4.6",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.7.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "history": "^5.3.0",
    "husky": "^7.0.4",
    "jest": "27.4.7",
    "msw": "^0.39.2",
    "node-fetch": "^3.2.3",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "4.5.4"
  }
}
